{
  "extends": "../../tslint.json",
  "rules": {
    "directive-selector": [
      true,
      "attribute",
      "camelCase"
    ],
    "component-selector": [
      true,
      "element",
      "kebab-case"
    ],
    "import-blacklist": [
      true,
      [
        "^@nestjs.*"
      ]
    ]
  }
}
